<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    
    <title>Cinemagraph</title>

    <link href="{{ toolbox-css }}" rel="stylesheet">
    <link href="assets/css/styles.css" rel="stylesheet">{{! never remove this line, css will be inlined here }}
</head>
<body>

    <we-wallpaper>

        <we-video 
            onplayback="onPlaying()" 
            loop 
            autoplay 
            muted 
            fullscreen 
            disable-vast
        >
            <source src="assets/videos/loop.mp4" type="video/mp4"/>
        </we-video>

        <we-clickbutton></we-clickbutton>

    </we-wallpaper>

    <script src="{{ wallpaper-api }}" type="text/javascript"></script>
    <script src="{{ toolbox-js }}" type="text/javascript"></script>
    <script src="./assets/js/scripts.js" type="text/javascript"></script>{{! never remove this line, js will be inlined here }}
</body>
</html>